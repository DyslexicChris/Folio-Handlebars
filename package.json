{
    "name": "FolioHandlebars",
    "version": "0.0.1",
    "description": "Folio.js handlebars render engine",
    "main": "index.js",
    "scripts": {
        "test": "npm run unit-tests",
        "unit-tests": "./node_modules/mocha/bin/mocha ./test/unit -R spec --recursive",
        "coveralls": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --report lcovonly -- ./test/unit -R dot --recursive && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
        "generate-docs": "rm -r ./jsdoc/*; echo \"Generating Documentation...\\n\"; ./node_modules/.bin/jsdoc -r -d ./jsdoc ./lib"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/DyslexicChris/FolioHandlebars.git"
    },
    "keywords": [
        "folio.js",
        "folio",
        "handlebars",
        "mustache",
        "render",
        "view",
        "templating",
        "template",
        "html",
        "engine"
    ],
    "author": "DyslexicChris <npm@dyslexicchris.co.uk> (https://github.com/DyslexicChris)",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/DyslexicChris/FolioHandlebars/issues"
    },
    "homepage": "https://github.com/DyslexicChris/FolioHandlebars",
    "dependencies": {
        "handlebars": "3.0.0"
    },
    "devDependencies": {
        "chai": "^2.1.0",
        "istanbul": "^0.3.6",
        "jsdoc": "^3.3.0-beta1",
        "mockery": "^1.4.0",
        "mocha": "^2.1.0",
        "sinon": "^1.12.2",
        "coveralls": "^2.11.2"
    }
}
